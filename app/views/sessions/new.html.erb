<%= render 'shared/title', header: "Study Group", sub_header: "login" %>

<div class='well'>
	<%= form_tag "/login" %>
		
		<div class='control-group'>
		<%= label_tag "username" %>
		<%= text_field_tag "username" %>
		</div>

		<div class='control-group'>
		<%= label_tag "password" %>
		<%= password_field_tag "password" %>
		</div>

		<%= submit_tag "Log in", class: "btn btn-success" %>
</div>